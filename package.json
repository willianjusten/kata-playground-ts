{
  "name": "kata-playground-ts",
  "version": "1.0.0",
  "description": "A playground to write kata challenges in typescript.",
  "main": "index.js",
  "scripts": {
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watchAll --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "lint": "eslint src --ext .ts",
    "new:kata": "yarn plop --plopfile generators/plopfile.js"
  },
  "keywords": [
    "kata",
    "challenges",
    "interview",
    "typescript"
  ],
  "author": "Willian Justen <willianjustenqui@gmail.com>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-jest": "^27.0.2",
    "eslint": "^7.28.0",
    "jest": "^27.0.4",
    "plop": "^2.7.4",
    "prettier": "^2.3.1",
    "typescript": "^4.3.2"
  }
}
